# compiler flags.  -g for debug, -O3 for optimization
CFLAGS=-g -O3

# first target is built by default
all: tester lab1

# include file dependencies
history.o: history.h
tokenizer.o: tokenizer.h
tester.o: history.h tokenizer.h
lab1.o: history.h tokenizer.h

# $@=target, $^=prerequisites
tester: history.o tokenizer.o tester.c
	cc -o $@ $^ $(CFLAGS)
lab1: lab1.c
	cc -o $@ $^ $(CFLAGS)

# deletes files generated by compilation
clean:
	rm -f *.o tester lab1
